{
    "env": {
        "browser": true,
        "es2022": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "off",
        "class-methods-use-this": ["error", 
            { 
                "exceptMethods": ["validateUserName"] 
                }]
    },
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    }
}